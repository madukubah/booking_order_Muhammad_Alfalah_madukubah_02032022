<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="work_order_cancel_form" model="ir.ui.view">
            <field name="name">work.order.cancel.form</field>
            <field name="model">work.order.cancel</field>
            <field name="arch" type="xml">
                <form >
                    <group>
                        <field name="note"/>
                        <field name="work_order_id" invisible='1' />
                    </group>
                <footer>
                    <button name="action_ok" string="Ok" type="object" default_focus="1" class="oe_highlight"/> 
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
